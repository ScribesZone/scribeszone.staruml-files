{
  "reload" : "true",
  "processors" : [
    {
      "extension": [".odp",".odt",".odx"],
      "command": "soffice {{filename}}"
    },
    {
      "extension": ".use",
      "command": "use  {{filename}}"
    },
    {
      "extension": [
        ".gls", ".cl1", ".ob1", ".sc1", ".pas", ".uss", ".pes",
        ".tas", ".aus", ".res",

        ".md", ".rst",

        ".java",".c", ".h", ".js", ".ts", ".html", ".css"],
      "command": "code {{filename}}"
    },
    {
      "command": "xdg-open file:///{{filename}}"
    }
  ]
}